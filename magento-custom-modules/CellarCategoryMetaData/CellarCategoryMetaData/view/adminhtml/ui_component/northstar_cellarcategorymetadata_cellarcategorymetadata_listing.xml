<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">northstar_cellarcategorymetadata_cellarcategorymetadata_listing.northstar_cellarcategorymetadata_cellarcategorymetadata_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>northstar_cellarcategorymetadata_cellarcategorymetadata_columns</spinner>
		<deps>
			<dep>northstar_cellarcategorymetadata_cellarcategorymetadata_listing.northstar_cellarcategorymetadata_cellarcategorymetadata_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add New Categorization</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="northstar_cellarcategorymetadata_cellarcategorymetadata_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">cellarcategorymetadata_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Northstar_CellarCategoryMetaData::cellarcategorymetadata</aclResource>
		<dataProvider name="northstar_cellarcategorymetadata_cellarcategorymetadata_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>cellarcategorymetadata_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="northstar_cellarcategorymetadata_cellarcategorymetadata_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">northstar_cellarcategorymetadata_cellarcategorymetadata_listing.northstar_cellarcategorymetadata_cellarcategorymetadata_listing.northstar_cellarcategorymetadata_cellarcategorymetadata_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">cellarcategorymetadata_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="northstar_cellarcategorymetadata/cellarcategorymetadata/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">northstar_cellarcategorymetadata_cellarcategorymetadata_listing.northstar_cellarcategorymetadata_cellarcategorymetadata_listing.northstar_cellarcategorymetadata_cellarcategorymetadata_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>cellarcategorymetadata_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="cellarcategorymetadata_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="customwinecellarid_id">
			<settings>
				<filter>text</filter>
				<label translate="true">Cellar ID</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
<column name="category_id" class="Northstar\CellarCategoryMetaData\Ui\Component\Listing\Column\CategoryLabel">
    <settings>
        <label translate="true">Category</label>
        <sortable>false</sortable>
    </settings>
</column>
		<column name="cellar_title">
			<settings>
				<filter>text</filter>
				<label translate="true">Title</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="cellar_description">
			<settings>
				<filter>text</filter>
				<label translate="true"> Description</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="cellar_main_image">
			<settings>
				<filter>text</filter>
				<label translate="true">Thumbnail Image</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<actionsColumn name="actions" class="Northstar\CellarCategoryMetaData\Ui\Component\Listing\Column\CellarcategorymetadataActions">
			<settings>
				<indexField>cellarcategorymetadata_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
